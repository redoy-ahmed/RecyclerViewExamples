<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redoy.recyclerviewselection">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.redoy.activity.MultipleViewTypeActivity"></activity>
        <activity android:name="com.redoy.activity.NormalRecyclerViewActivity" />
        <activity android:name="com.redoy.activity.SwipeSelectionActivity" />
        <activity android:name="com.redoy.activity.MultipleSelectionActivity" />
        <activity android:name="com.redoy.activity.SingleSelectionActivity" />
        <activity android:name="com.redoy.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.redoy.activity.CardRecyclerViewActivity" />
    </application>

</manifest>